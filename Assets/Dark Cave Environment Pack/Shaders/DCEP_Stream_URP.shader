			#endif
			CBUFFER_END
			TEXTURE2D(_Ripples_Displacement);
			TEXTURE2D(_Ripples2);
			SAMPLER(sampler_Ripples_Displacement);
			TEXTURE2D(_AlbedoMap);
			TEXTURE2D(_Ripples);
